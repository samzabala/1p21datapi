		"autoprefixer": "^10.2.5",
		"cssnano": "^5.0.17",
		"dart-sass": "^1.25.0",
		"eslint": "^8.11.0",
		"postcss": "^8.4.6",
		"postcss-cli": "^9.1.0",
		"prettier": "^2.2.1",
		"rollup": "^2.41.4",
		"sass": "^1.33.0",
		"watch": "^0.13.0"
	},
